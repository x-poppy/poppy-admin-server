{
  "name": "@x-poppy/poppy-server",
  "version": "0.0.1",
  "main": "dist/main.js",
  "scripts": {
    "start": "nodemon --exec 'ts-node -r tsconfig-paths/register ./src/main.ts --files'",
    "prebuild": "rimraf dist",
    "build": "tsc --build ./tsconfig.json",
    "postbuild": "copyfiles config/** locales/** package.json LICENSE swagger.yml dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier src --write",
    "test": "jest --passWithNoTests --coverage src",
    "test:dev": "jest --watch --coverage src",
    "prepare": "husky install"
  },
  "files": [
    "dist"
  ],
  "author": "X-Poppy",
  "license": "MIT",
  "dependencies": {
    "@augejs/axios": "1.6.1",
    "@augejs/core": "1.0.8",
    "@augejs/file-config": "1.6.1",
    "@augejs/i18n": "1.6.1",
    "@augejs/koa": "1.6.1",
    "@augejs/koa-access-token": "1.4.7",
    "@augejs/koa-bodyparser": "1.4.7",
    "@augejs/koa-limit-request": "1.4.7",
    "@augejs/koa-security": "1.4.7",
    "@augejs/koa-session-token": "1.4.7",
    "@augejs/koa-static": "1.4.7",
    "@augejs/koa-swagger": "^1.4.7",
    "@augejs/log4js": "1.6.1",
    "@augejs/mail": "1.6.1",
    "@augejs/typeorm": "1.6.1",
    "@augejs/validator": "1.6.1",
    "@augejs/views": "1.6.1",
    "bcrypt": "^5.0.1",
    "ejs": "^3.1.6",
    "mysql": "2.18.1",
    "nodejs-snowflake": "1.6.2",
    "secure-random-password": "^0.2.3",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^26.0.23",
    "@types/secure-random-password": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "commitlint": "^13.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-cucumber": "^3.0.1",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
