{
  "name": "@x-poppy/admin-server",
  "version": "0.0.1",
  "main": "dist/main.js",
  "scripts": {
    "start": "nodemon --exec 'npm run apidoc && ts-node -r tsconfig-paths/register ./src/main.ts --files'",
    "prebuild": "rimraf dist",
    "build": "npm run apidoc && tsc --build ./tsconfig.json",
    "postbuild": "copyfiles config/** locales/** package.json package-lock.json dist && cp -R public dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write",
    "test": "jest --passWithNoTests --coverage",
    "test:dev": "jest --watch --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "apidoc": "rimraf public/apidoc && apidoc -i src/ -t apidoc/template -f \".*\\.ts$\" -o public/apidoc",
    "prepare": "husky install",
    "changelog:first": "standard-changelog -f",
    "changelog": "standard-changelog"
  },
  "files": [
    "dist"
  ],
  "author": "X-Poppy",
  "license": "MIT",
  "dependencies": {
    "@augejs/axios": "1.3.9",
    "@augejs/core": "1.0.8",
    "@augejs/file-config": "1.3.9",
    "@augejs/i18n": "1.3.9",
    "@augejs/koa": "1.3.9",
    "@augejs/koa-bodyparser": "1.1.0",
    "@augejs/koa-security": "1.1.0",
    "@augejs/koa-static": "1.1.0",
    "@augejs/log4js": "1.3.9",
    "@augejs/mail": "1.3.9",
    "@augejs/typeorm": "1.3.9",
    "@augejs/views": "1.3.9",
    "ejs": "^3.1.6",
    "mysql": "2.18.1",
    "nodejs-snowflake": "1.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "commitlint": "^12.1.4",
    "conventional-changelog-cli": "^2.1.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "standard-changelog": "^2.0.27",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.2.0"
  }
}
