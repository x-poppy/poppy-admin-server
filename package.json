{
  "name": "@x-poppy/poppy-server",
  "version": "0.0.1",
  "main": "dist/main.js",
  "scripts": {
    "start": "nodemon --exec 'ts-node -r tsconfig-paths/register ./src/main.ts --files'",
    "prebuild": "rimraf dist",
    "build": "tsc --build ./tsconfig.json",
    "postbuild": "copyfiles config/** locales/** package.json package-lock.json swagger.yml dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write",
    "test": "jest --passWithNoTests --coverage src",
    "test:dev": "jest --watch --coverage src",
    "prepare": "husky install"
  },
  "files": [
    "dist"
  ],
  "author": "X-Poppy",
  "license": "MIT",
  "dependencies": {
    "@augejs/axios": "1.4.0",
    "@augejs/core": "1.0.8",
    "@augejs/file-config": "1.4.0",
    "@augejs/i18n": "1.4.0",
    "@augejs/koa": "1.4.0",
    "@augejs/koa-access-token": "1.3.4",
    "@augejs/koa-bodyparser": "1.3.4",
    "@augejs/koa-security": "1.3.4",
    "@augejs/koa-static": "1.3.4",
    "@augejs/log4js": "1.4.0",
    "@augejs/mail": "1.4.0",
    "@augejs/typeorm": "1.4.0",
    "@augejs/views": "1.4.0",
    "ejs": "^3.1.6",
    "mysql": "2.18.1",
    "nodejs-snowflake": "1.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "commitlint": "^12.1.4",
    "copyfiles": "^2.4.1",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-cucumber": "^3.0.1",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.5"
  }
}
